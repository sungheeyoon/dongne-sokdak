# 스테이징 환경 설정
ENVIRONMENT=staging

# 데이터베이스 설정 (스테이징 DB로 변경 필요)
DATABASE_URL=postgresql://postgres:STAGING_PASSWORD@db.staging-project.supabase.co:5432/postgres

# Supabase 설정 (스테이징 프로젝트로 변경 필요)
SUPABASE_URL=https://staging-project.supabase.co
SUPABASE_KEY=your-staging-supabase-key

# JWT 보안 설정 (스테이징용 시크릿)
JWT_SECRET=your-staging-jwt-secret-key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS 설정 (스테이징 도메인)
CORS_ORIGINS=https://staging.yourdomain.com

# 로깅 설정
LOG_LEVEL=INFO

# Sentry 설정
SENTRY_DSN=your-sentry-dsn-for-staging