# 운영 환경 설정
ENVIRONMENT=production

# 데이터베이스 설정 (운영 DB로 변경 필요)
DATABASE_URL=postgresql://postgres:PRODUCTION_PASSWORD@db.production-project.supabase.co:5432/postgres

# Supabase 설정 (운영 프로젝트로 변경 필요)
SUPABASE_URL=https://production-project.supabase.co
SUPABASE_KEY=your-production-supabase-key

# JWT 보안 설정 (운영용 강력한 시크릿)
JWT_SECRET=your-production-jwt-secret-key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# CORS 설정 (운영 도메인만 허용)
CORS_ORIGINS=https://dongne-sokdak.vercel.app,http://localhost:3000

# 로깅 설정
LOG_LEVEL=ERROR

# Sentry 설정 (선택사항 - 실제 DSN으로 교체)
# SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id